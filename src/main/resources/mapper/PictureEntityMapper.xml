<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.monster.blog.mapper.PictureMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.monster.blog.domain.PictureEntity">
        <id column="ID" property="id" />
        <result column="PICTURE_NAME" property="pictureName" />
        <result column="PICTURE_TIME" property="pictureTime" />
        <result column="PICTURE_URL" property="pictureUrl" />
        <result column="DESCS" property="descs" />
        <result column="IS_REMOVED" property="isRemoved" />
        <result column="CREATED_TIME" property="createdTime" />
        <result column="CREATED_BY" property="createdBy" />
        <result column="UPDATED_TIME" property="updatedTime" />
        <result column="UPDATED_BY" property="updatedBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, PICTURE_NAME, PICTURE_TIME, PICTURE_URL, DESCS, IS_REMOVED, CREATED_TIME, CREATED_BY, UPDATED_TIME, UPDATED_BY
    </sql>

</mapper>
